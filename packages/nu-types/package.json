{
  "name": "@nextunicorn/types",
  "packageManager": "yarn@3.2.2",
  "version": "0.0.6",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/nextunicorn-inc/nu-packages",
  "bugs": "https://github.com/nextunicorn-inc/nu-packages/issues",
  "repository": "git://github.com/nextunicorn-inc/nu-packages.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "sideEffects": false,
  "author": "Nextunicorn Inc",
  "engines": {
    "node": "^16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "build": "yarn clean && vite build",
    "watch": "vite build --watch",
    "clean": "rimraf dist",
    "update": "yarn build && npm publish --access=public"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^1.4.1"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
