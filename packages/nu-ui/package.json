{
  "name": "@nextunicorn/ui",
  "packageManager": "yarn@3.2.2",
  "version": "0.0.20",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/nextunicorn-inc/nu-packages",
  "bugs": "https://github.com/nextunicorn-inc/nu-packages/issues",
  "repository": "git://github.com/nextunicorn-inc/nu-packages.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "variable.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./css": {
      "import": "./dist/variables.css",
      "require": "./dist/variables.css"
    }
  },
  "author": "Nextunicorn Inc",
  "engines": {
    "node": "^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "dev": "start-storybook -p 4000",
    "build": "yarn clean && cp src/@styles/variables.css dist/variables.css",
    "watch": "tsc --build -w",
    "clean": "rimraf dist storybook-static && tsc --build --verbose",
    "build:storybook": "build-storybook",
    "update": "yarn build && npm publish --access=public"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@nextunicorn/icons": "^0.0.1",
    "@nextunicorn/types": "^0.0.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/core-common": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@svgr/webpack": "^6.3.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.7.4",
    "classnames": "^2.3.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "rc-checkbox": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.4",
    "typescript-styled-plugin": "^0.18.2"
  },
  "bundledDependencies": true,
  "peerDependencies": {
    "@nextunicorn/types": "*",
    "@types/react": "^18.0.0",
    "rc-checkbox": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "require-from-string": "*",
    "webpack": "*"
  }
}
