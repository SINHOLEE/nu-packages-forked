  "scripts": {
    "dev": "start-storybook -p 4000",
    "1build": "yarn clean && cp @styles/variables.css dist/variables.css",
    "watch": "tsc --build -w",
    "clean": "rimraf dist storybook-static lib && tsc --build --verbose",
    "build:storybook": "build-storybook",
    "build:lib": "node ./scripts/compileLib.js",
    "build:js": "node ./scripts/compileDist.js",
    "build": "yarn clean && yarn build:lib && yarn build:js",
    "update": "yarn build && npm publish --access=public"
